{
  "expo": {
    "jsEngine": "hermes",
    "name": "EFTM",
    "slug": "eftm",
    "scheme": "eftm",
    "description": "EFTM is the home of Tech Cars and Lifestyle news and reviews in Australia. The EFTM app is a simple app to for you to catch up with our latest news and podcasts.",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.5.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.eftm.app",
      "supportsTablet": false,
      "buildNumber": "1.0.0"
    },
    "android": {
      "versionCode": 3,
      "package": "com.eftm.app",
      "intentFilters": [
  {
    "action": "VIEW",
    "data": [
      {
        "scheme": "https",
        "host": "*.eftm.com",
        "pathPrefix": "/"
      }
    ],
    "category": [
      "BROWSABLE",
      "DEFAULT"
    ]
  }
],
    },
    "androidStatusBar": {
            "translucent": true
        },
    "web": {
      "themeColor": "#000000"
    },
    "plugins": [
      "onesignal-expo-plugin"
    ],
    "extra": {
      "oneSignalAppId": "c93f5d78-73ff-4a87-8e82-45ccf668c290"
    },
    "notification": {
      "androidMode": "default"
    }
  }
}
